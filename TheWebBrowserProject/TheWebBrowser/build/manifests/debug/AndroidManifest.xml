<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="1" android:versionName="1.0" package="com.ivinny.thewebbrowser">

    <uses-sdk android:minSdkVersion="7" android:targetSdkVersion="16"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application android:allowBackup="true" android:icon="@drawable/world" android:label="@string/app_name" android:theme="@android:style/Theme.Holo">
        <activity android:label="@string/app_name" android:name="com.ivinny.thewebbrowser.MainActivity">
            <intent-filter>

                <action android:name="android.intent.action.VIEW"/>

                <category android:name="android.intent.category.DEFAULT"/>
                <data android:scheme="http"/>
            </intent-filter>

            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>

           </intent-filter>
        </activity>
    </application>

</manifest>
